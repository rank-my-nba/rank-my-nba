{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "es5",
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["dom", "ESNext"],
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./"
  },
  "exclude": ["node_modules"]
}
